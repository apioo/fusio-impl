{
  "status": 1,
  "path": "\/backend\/schema\/$schema_id<[0-9]+>",
  "pathParameters": "Schema_Entity_Path",
  "methods": {
    "GET": {
      "security": {
        "backend": [
          "backend.schema"
        ]
      },
      "tags": [],
      "responses": {
        "200": "Schema"
      }
    },
    "PUT": {
      "security": {
        "backend": [
          "backend.schema"
        ]
      },
      "tags": [],
      "request": "Schema_Update",
      "responses": {
        "200": "Message"
      }
    },
    "DELETE": {
      "security": {
        "backend": [
          "backend.schema"
        ]
      },
      "tags": [],
      "responses": {
        "200": "Message"
      }
    }
  },
  "definitions": {
    "Message": {
      "type": "object",
      "properties": {
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "Schema": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "status": {
          "type": "integer"
        },
        "name": {
          "type": "string",
          "pattern": "^[a-zA-Z0-9\\-\\_]{3,255}$"
        },
        "source": {
          "$ref": "Schema_Source"
        },
        "form": {
          "$ref": "Schema_Form"
        }
      }
    },
    "Schema_Entity_Path": {
      "type": "object",
      "properties": {
        "schema_id": {
          "type": "integer"
        }
      }
    },
    "Schema_Form": {
      "description": "Contains a UI vocabulary to describe the UI of the schema",
      "type": "object",
      "additionalProperties": {
        "type": "any"
      }
    },
    "Schema_Source": {
      "type": "object",
      "additionalProperties": {
        "type": "any"
      }
    },
    "Schema_Update": {
      "$extends": "Schema",
      "type": "object"
    }
  }
}