{
  "status": 1,
  "path": "\/backend\/rate\/$rate_id<[0-9]+>",
  "pathParameters": "Rate_Entity_Path",
  "methods": {
    "GET": {
      "security": {
        "backend": [
          "backend.rate"
        ]
      },
      "tags": [],
      "responses": {
        "200": "Rate"
      }
    },
    "PUT": {
      "security": {
        "backend": [
          "backend.rate"
        ]
      },
      "tags": [],
      "request": "Rate_Update",
      "responses": {
        "200": "Message"
      }
    },
    "DELETE": {
      "security": {
        "backend": [
          "backend.rate"
        ]
      },
      "tags": [],
      "responses": {
        "200": "Message"
      }
    }
  },
  "definitions": {
    "Message": {
      "type": "object",
      "properties": {
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "Rate": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "priority": {
          "type": "integer",
          "minimum": 0
        },
        "name": {
          "type": "string",
          "pattern": "^[a-zA-Z0-9\\-\\_]{3,64}$"
        },
        "rateLimit": {
          "type": "integer",
          "minimum": 0
        },
        "timespan": {
          "format": "duration",
          "type": "string"
        },
        "allocation": {
          "type": "array",
          "items": {
            "$ref": "Rate_Allocation"
          }
        }
      }
    },
    "Rate_Allocation": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "routeId": {
          "type": "integer"
        },
        "appId": {
          "type": "integer"
        },
        "authenticated": {
          "type": "boolean"
        },
        "parameters": {
          "type": "string"
        }
      }
    },
    "Rate_Entity_Path": {
      "type": "object",
      "properties": {
        "rate_id": {
          "type": "integer"
        }
      }
    },
    "Rate_Update": {
      "$extends": "Rate",
      "type": "object"
    }
  }
}