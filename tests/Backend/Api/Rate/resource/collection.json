{
  "status": 1,
  "path": "\/backend\/rate",
  "methods": {
    "GET": {
      "security": {
        "backend": [
          "backend.rate"
        ]
      },
      "tags": [],
      "queryParameters": "Rate_Collection_Query",
      "responses": {
        "200": "Rate_Collection"
      }
    },
    "POST": {
      "security": {
        "backend": [
          "backend.rate"
        ]
      },
      "tags": [],
      "request": "Rate_Create",
      "responses": {
        "201": "Message"
      }
    }
  },
  "definitions": {
    "Collection": {
      "type": "object",
      "properties": {
        "totalResults": {
          "type": "integer"
        },
        "startIndex": {
          "type": "integer"
        },
        "entry": {
          "type": "array",
          "items": {
            "$generic": "T"
          }
        }
      }
    },
    "Message": {
      "type": "object",
      "properties": {
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "Rate": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "priority": {
          "type": "integer",
          "minimum": 0
        },
        "name": {
          "type": "string",
          "pattern": "^[a-zA-Z0-9\\-\\_]{3,64}$"
        },
        "rateLimit": {
          "type": "integer",
          "minimum": 0
        },
        "timespan": {
          "format": "duration",
          "type": "string"
        },
        "allocation": {
          "type": "array",
          "items": {
            "$ref": "Rate_Allocation"
          }
        }
      }
    },
    "Rate_Allocation": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "routeId": {
          "type": "integer"
        },
        "appId": {
          "type": "integer"
        },
        "authenticated": {
          "type": "boolean"
        },
        "parameters": {
          "type": "string"
        }
      }
    },
    "Rate_Collection": {
      "$ref": "Collection",
      "$template": {
        "T": "Rate"
      }
    },
    "Rate_Collection_Query": {
      "type": "object",
      "properties": {
        "startIndex": {
          "type": "integer"
        },
        "count": {
          "type": "integer"
        },
        "search": {
          "type": "string"
        }
      }
    },
    "Rate_Create": {
      "$extends": "Rate",
      "type": "object",
      "required": [
        "name"
      ]
    }
  }
}