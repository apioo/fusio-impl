{
  "status": 1,
  "path": "\/backend\/marketplace",
  "methods": {
    "GET": {
      "security": {
        "backend": [
          "backend.marketplace"
        ]
      },
      "tags": [],
      "responses": {
        "200": "Marketplace_Collection"
      }
    },
    "POST": {
      "security": {
        "backend": [
          "backend.marketplace"
        ]
      },
      "tags": [],
