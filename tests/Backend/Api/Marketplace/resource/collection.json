        },
        "description": {
          "type": "string"
        },
        "screenshot": {
          "type": "string"
        },
        "website": {
          "type": "string"
        },
        "downloadUrl": {
          "type": "string"
        },
        "sha1Hash": {
          "type": "string"
        }
      }
    },
    "Marketplace_Collection": {
      "type": "object",
      "properties": {
        "apps": {
          "$ref": "Marketplace_Collection_Apps"
        }
      }
    },
    "Marketplace_Collection_Apps": {
      "type": "object",
      "additionalProperties": {
        "$ref": "Marketplace_Remote_App"
      }
    },
    "Marketplace_Install": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        }
      }
    },
    "Marketplace_Remote_App": {
      "$extends": "Marketplace_App",
      "type": "object",
      "properties": {
        "local": {
