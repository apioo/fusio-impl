{
  "status": 1,
  "path": "\/backend\/action\/list",
  "methods": {
    "GET": {
      "security": {
        "backend": [
          "backend.action"
        ]
      },
      "tags": [],
      "responses": {
        "200": "Action_Index"
      }
    }
  },
  "definitions": {
    "Action": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "status": {
          "type": "integer"
        },
        "name": {
          "type": "string",
          "pattern": "^[a-zA-Z0-9\\-\\_]{3,255}$"
        },
        "class": {
          "type": "string"
        },
        "engine": {
          "type": "string"
        },
        "config": {
          "$ref": "Action_Config"
        }
      }
    },
    "Action_Config": {
      "type": "object",
      "additionalProperties": {
        "type": "any"
      }
    },
    "Action_Index": {
      "type": "object",
      "properties": {
        "actions": {
          "type": "array",
          "items": {
            "$ref": "Action"
          }
        }
      }
    }
  }
}
