{
  "status": 1,
  "path": "\/consumer\/plan\/contract",
  "methods": {
    "GET": {
      "security": {
        "consumer": [
          "consumer.plan"
        ]
      },
      "tags": [],
      "responses": {
        "200": "Plan_Contract_Collection"
      }
    },
    "POST": {
      "security": {
        "consumer": [
          "consumer.plan"
        ]
      },
      "tags": [],
      "request": "Plan_Order_Request",
      "responses": {
        "201": "Plan_Order_Response"
      }
    }
  },
  "definitions": {
    "Collection": {
      "type": "object",
      "properties": {
        "totalResults": {
          "type": "integer"
        },
        "startIndex": {
          "type": "integer"
        },
        "entry": {
          "type": "array",
          "items": {
            "$generic": "T"
          }
        }
      }
    },
    "Plan": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "price": {
          "type": "number"
        },
        "points": {
          "type": "integer"
        }
      }
    },
    "Plan_Contract": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "status": {
          "type": "integer"
        },
        "plan": {
          "$ref": "Plan"
        },
        "amount": {
          "type": "number"
        },
        "points": {
          "type": "integer"
        },
        "period": {
          "type": "integer"
        },
        "invoices": {
          "type": "array",
          "items": {
            "$ref": "Plan_Invoice"
          }
        },
        "insertDate": {
          "format": "date-time",
          "type": "string"
        }
      }
    },
    "Plan_Contract_Collection": {
      "$ref": "Collection",
      "$template": {
        "T": "Plan_Contract"
      }
    },
    "Plan_Invoice": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "status": {
          "type": "integer"
        },
        "amount": {
          "type": "number"
        },
        "points": {
          "type": "integer"
        },
        "payDate": {
          "format": "date-time",
          "type": "string"
        },
        "insertDate": {
          "format": "date-time",
          "type": "string"
        }
      }
    },
    "Plan_Order_Request": {
      "type": "object",
      "properties": {
        "planId": {
          "type": "integer"
        }
      },
      "required": [
        "planId"
      ]
    },
    "Plan_Order_Response": {
      "type": "object",
      "properties": {
        "contractId": {
          "type": "integer"
        },
        "invoiceId": {
          "type": "integer"
        }
      }
    }
  }
}